{
  "name": "remesh-yjs",
  "version": "3.1.0",
  "description": "Yjs adapter for remesh",
  "homepage": "https://remesh-js.github.io/remesh/dist/index.html",
  "repository": "github:remeshjs/remesh",
  "bugs": "https://github.com/remesh-js/remesh/issues",
  "author": "https://github.com/Lucifier129",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "module": "esm/index.mjs",
  "exports": {
    ".": {
      "types": "./cjs/index.d.ts",
      "import": "./esm/index.mjs",
      "require": "./cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "scripts": {
    "esno": "esno src/index.ts"
  },
  "peerDependencies": {
    "remesh": "^3.1.0",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.1",
    "yjs": "^13.5.41"
  },
  "devDependencies": {
    "@types/node": "^16.7.2",
    "remesh": "^3.1.0",
    "rxjs": "^7.0.0",
    "typescript": "^4.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
